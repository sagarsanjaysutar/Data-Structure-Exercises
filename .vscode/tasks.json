{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "GST Build",
      "command": "/usr/bin/g++",
      "args": [
        "-fdiagnostics-color=always",
        "-g",
        "${file}",
        "-o",
        "${workspaceFolder}/Output/${fileBasenameNoExtension}.o",
        "`pkg-config --cflags --libs gstreamer-1.0 gstreamer-pbutils-1.0`"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": ["$gcc"],
      "group": "build",
      "detail": "Task generated by Debugger."
    },
    {
      "type": "cppbuild",
      "label": "Normal C++ Build",
      "command": "g++",
      "args": [
        "-g",
        "${file}",
        "-std=c++11",
        "-o",
        "${workspaceFolder}/Output/${fileBasenameNoExtension}.exe"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": ["$gcc"],
      "detail": "Task generated by Debugger."
    },
    {
      "label": "Normal C++ Run",
      "type": "shell",
      "command": "${workspaceFolder}/Output/${fileBasenameNoExtension}.exe",
      "options": {
        "cwd": "${fileDirname}"
      },
      "group": {
        "kind": "build",
        "isDefa -lult": true
      },
      "presentation": {
        "reveal": "always",
        "panel": "new"
      },
      "problemMatcher": []
    },
    {
      "label": "GST Build and Run",
      "dependsOrder": "sequence",
      "dependsOn": ["GST Build", "Normal C++ Run"],
      "group": {
        "kind": "build"
      }
    },
    {
      "label": "Normal Build and Run",
      "dependsOrder": "sequence",
      "dependsOn": ["Normal C++ Build", "Normal C++ Run"],
      "group": {
        "kind": "build"
      }
    }
  ],
  "version": "2.0.0"
}
